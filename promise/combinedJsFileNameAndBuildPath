				size: 'sm',
				resolve: {
					nameData: function () {
						return dataKeuangan.jenis;
					},
					namaEvent: function () {
						return 'hapus';
					}
				}
			});
			segmentasimodalinstance.result.then(function () {
				form.dataKeuanganList.splice(index, 1);
				$scope.tableKeuanganVendor.reload();
			});
		}

		$scope.nextDataPengalaman = function () {
