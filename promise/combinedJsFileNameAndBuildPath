		/* ---------------------------------------------------- DATA PENGALAMAN ------------------------------------------------ */


		$scope.checked = function (pengalamanStatus) {
			if (pengalamanStatus) {
				$scope.tanpaPengalaman = true;
			} else {
				$scope.tanpaPengalaman = false;
			}
			//console.log("TANPA PENGALAMAN = "+$scope.tanpaPengalaman);
		}


		form.dataPengalamanPekerjaanList = [];
